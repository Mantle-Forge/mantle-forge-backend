# MantleForge Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Your backend's deployer wallet private key (without 0x prefix)
# This wallet will be used to deploy agent contracts
// 1768471032389
BACKEND_PRIVATE_KEY=your-private-key-here

# The RPC URL for the Mantle Sepolia Testnet
# This is where your contracts are deployed
MANTLE_RPC_URL=https://rpc.sepolia.mantle.xyz

# The factory address from deployment
# Deployed AgentFactory contract address on Mantle Sepolia
AGENT_FACTORY_ADDRESS=0xffad39924ff5cdaacd04350ed6ee960a2394a065

# A strong, random string for encrypting secrets
# Generate a secure random string (32+ characters recommended)
# You can use: openssl rand -base64 32
MASTER_SECRET_KEY=your-master-secret-key-here

# Server Configuration
PORT=3000

# Optional: GitHub OAuth (for automatic webhook setup - Vercel-like)
# GITHUB_CLIENT_ID=your-github-oauth-client-id
# GITHUB_CLIENT_SECRET=your-github-oauth-client-secret
# BACKEND_URL=https://mantle-git-agent.onrender.com

# Optional: GitHub Webhook Secret (for webhook verification)
# GITHUB_WEBHOOK_SECRET=your-webhook-secret-here

# Optional: Database path (defaults to ./db.sqlite)
# DB_PATH=./db.sqlite


